                        <tbody>
                        <?php foreach ($this->ownquotes as $p) { ?>
                            <tr class='status_<?= $p['status'] ?> approval_<?= $p['approve_status'] ?>'>
                                <td><a a target='_blank' href="/quote/edit/id/<?= $p['quote_id'] ?>"><?= $p['quote_id'] ?></a></td>
                                <td><a a target='_blank' href="/project/edit/id/<?= $p['project_id'] ?>"><?= $p['project_name'] ?></a></td>
                                <td><?= $p['quote_date']->format('Y-m-d') ?></td>
                                <td><span class='<?php if ($p['expire_date']->format('Ymd') <= date('Ymd')) echo 'red'; ?>'><?= $p['expire_date']->format('Y-m-d') ?></span></td>
                                <td><span><?php if ($p['ship_required_date'] != null && $p['ship_required_date']->format('Y-m-d') > '2000-01-01') {
                                                echo $p['ship_required_date']->format('Y-m-d');
                                            } ?></span></td>
                                <td><?= $p['status_name'] ?></td>
                                <?php if ($p['approve_status'] == -1) { ?>
                                    <td class='disapproved orange'>Disapproved</td>
                                <?php } else if ($p['approve_status'] == 10) { ?>
                                    <td class='approved green'>Approved</td>
                                <?php } else if ($p['approve_status'] == 1) { ?>
                                    <td class='waiting red'>Waiting Approval</td>
                                <?php } else { ?>
                                    <td class='init'>Not Submitted</td>
                                <?php } ?>
                                <td><?= $p['customer'] ?></td>
                                <td><!--<a href="#"><img src="images/icons/dark/close.png" alt="delete"></a>-->
                                    <a a target='_blank' href="/quote/edit/id/<?= $p['quote_id'] ?>"><img src="/images/icons/dark/pencil.png" alt="edit" style="margin:0px 4px"></a>
                                </td>
                            </tr>
                        <?php } ?>
                    </tbody>

                    <tbody>
                        <?php foreach ($this->memo as $l) { ?>
